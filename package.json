{
	"name": "weather-app",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"lint": "eslint src/**/*.js",
		"pretty": "prettier --write \"./**/*.{js,jsx,json}\""
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "yarn pretty && yarn lint"
		}
	},
	"dependencies": {
		"@material-ui/core": "^4.5.2",
		"axios": "^0.19.0",
		"convert-units": "^2.3.4",
		"moment": "^2.24.0",
		"prop-types": "^15.7.2",
		"react": "^16.10.2",
		"react-dom": "^16.10.2",
		"react-flexbox-grid": "^2.1.2",
		"react-redux": "^7.1.3",
		"react-scripts": "3.2.0",
		"react-weathericons": "^1.0.4",
		"redux": "^4.0.4"
	},
	"devDependencies": {
		"eslint": "^6.6.0",
		"husky": "^3.0.9",
		"prettier": "^1.19.1"
	}
}
